@book{Israelachvili2011,
abstract = {This chapter presents a study on polymer-mediated steric forces and thermal fluctuation forces. The steric forces between surfaces with end grafted chains are well understood both theoretically and experimentally, and are well defined. Thermal fluctuation forces are essentially entropic or osmotic in origin and are also referred to as entropically driven or protrusion forces. This chapter explains the purely repulsive steric force between two polymer-covered surfaces interacting in a liquid. It also details the concepts of polymer-associated interactions that are attractive or that modify steric interactions. The chapter begins with a discussion on diffuse interfaces in liquids. The states of polymers in solution and at surfaces are then explained. The chapter introduces the concepts of repulsive “steric” or “overlap” forces between polymer-covered surfaces. The chapter also discusses attractive “intersegment” and “bridging” forces and attractive “depletion” forces. Nonequilibrium aspects of polymer interactions are also discussed. The chapter concludes with a discussion on thermal fluctuations and forces between fluid-like interfaces.},
author = {Israelachvili, Jacob N.},
booktitle = {Intermolecular and Surface Forces},
doi = {10.1016/B978-0-12-375182-9.10016-8},
isbn = {9780123751829},
pages = {381--413},
publisher = {Elsevier},
title = {{Intermolecular and Surface Forces}},
url = {http://www.sciencedirect.com/science/article/pii/B9780123751829100168},
year = {2011}
}
