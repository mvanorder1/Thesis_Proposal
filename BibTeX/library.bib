keywords = {Kinetics,Metal Nanoparticles,Metal Nanoparticles: chemistry,Metal Nanoparticles: ultrastructure,Oxidation-Reduction,Physics,Surface Properties,Thermodynamics,growth,metal,nanocrystal,nucleation,shape control},
number = {1},
pages = {60--103},
pmid = {19053095},
title = {{Shape-Controlled Synthesis of Metal Nanocrystals : Simple Chemistry Meets Complex Physics?}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2791829{\&}tool=pmcentrez{\&}rendertype=abstract},
