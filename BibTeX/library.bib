file = {:C$\backslash$:/Users/Michael Van Order/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Raghunath, Flatau - 2015 - Study of magnetic domain evolution in an auxetic plane of Galfenol using Kerr microscopy.pdf:pdf},
issn = {0021-8979},
journal = {Journal of Applied Physics},
number = {17},
pages = {17E704},
title = {{Study of magnetic domain evolution in an auxetic plane of Galfenol using Kerr microscopy}},
