@book{Israelachvili2011s,
abstract = {This chapter elaborates the thermodynamic principles of self-assembly. The chapter begins with an introduction of soft structures, and then explains the fundamental thermodynamic equations of self-assembly. Conditions required for formation of aggregates are then discussed. The chapter elaborates effect of dimensionality and geometry on self-assembly. It presents a comparison study between infinite aggregates (phase separation) versus finite sized aggregates (micellization). The chapter formulates the basic equations of self-assembly in general statistical thermodynamic terms and then goes on to investigate the relevant intermolecular interactions that determine into which structures different amphiphiles will assemble. The chapter proceeds with discussing the role of molecular geometry in determining the structures formed. The chapter investigates how the geometric shape constraints of surfactant and lipid molecules restrict their assembly into aggregates of different shapes. The chapter concludes that once the aggregates' shape is known, many of their physical properties can be determined with the help of thermodynamic equations. Example of a dilute dispersion of (one-component) cylindrical micelles is given in support of this statement.},
author = {Israelachvili, Jacob N.},
booktitle = {Intermolecular and Surface Forces},
doi = {10.1016/B978-0-12-375182-9.10019-3},
isbn = {9780123751829},
pages = {503--534},
publisher = {Elsevier},
title = {{Intermolecular and Surface Forces}},
url = {http://www.sciencedirect.com/science/article/pii/B9780123751829100193},
