doi = {10.1109/TMAG.2011.2158303},
file = {:C$\backslash$:/Users/Michael Van Order/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Ueno, Yamada - 2011 - Performance of energy harvester using iron-gallium alloy in free vibration.pdf:pdf},
