@book{Israelachvili2011u,
abstract = {This chapter explores electrostatic interactions involving molecular polarization. Molecular polarization involves the dipole moments induced in molecules by the electric fields emanating from nearby molecules. For a nonpolar molecule, the polarizability arises from the displacement of its negatively charged electron cloud relative to the positively charged nucleus under the influence of an external electric field. The chapter lists the electronic polarizabilities of some common atoms and molecules. Since the electronic polarizability is associated with displacements of electron clouds, it is recognized that the polarizability of a molecule can be obtained by simply summing the characteristic polarizabilities of its covalent bonds. The chapter shows that apart from the purely Coulombic interaction between two charges or permanent dipoles in a vacuum, all other interactions are essentially polarization type interactions. The effects of polarization on electrostatic interactions are summarized in the chapter. The chapter concludes with a discussion on interactions between ions and uncharged molecules.},
author = {Israelachvili, Jacob N.},
booktitle = {Intermolecular and Surface Forces},
doi = {10.1016/B978-0-12-375182-9.10005-3},
isbn = {9780123751829},
pages = {91--106},
publisher = {Elsevier},
title = {{Intermolecular and Surface Forces}},
url = {http://www.sciencedirect.com/science/article/pii/B9780123751829100053},
year = {2011}
}
@article{Rocheleau2010,
abstract = {Cold, macroscopic mechanical systems are expected to behave contrary to our usual classical understanding of reality; the most striking and counterintuitive predictions involve the existence of states in which the mechanical system is located in two places simultaneously. Various schemes have been proposed to generate and detect such states, and all require starting from mechanical states that are close to the lowest energy eigenstate, the mechanical ground state. Here we report the cooling of the motion of a radio-frequency nanomechanical resonator by parametric coupling to a driven, microwave-frequency superconducting resonator. Starting from a thermal occupation of 480 quanta, we have observed occupation factors as low as 3.8 +/- 1.3 and expect the mechanical resonator to be found with probability 0.21 in the quantum ground state of motion. Further cooling is limited by random excitation of the microwave resonator and heating of the dissipative mechanical bath. This level of cooling is expected to make possible a series of fundamental quantum mechanical observations including direct measurement of the Heisenberg uncertainty principle and quantum entanglement with qubits.},
archivePrefix = {arXiv},
arxivId = {0907.3313},
author = {Rocheleau, T and Ndukum, T and Macklin, C and Hertzberg, J B and Clerk, a a and Schwab, K C},
doi = {10.1038/nature08681},
eprint = {0907.3313},
file = {:C$\backslash$:/Users/Michael Van Order/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Rocheleau et al. - 2010 - Preparation and detection of a mechanical resonator near the ground state of motion.pdf:pdf},
isbn = {0028-0836},
issn = {0028-0836},
journal = {Nature},
number = {7277},
pages = {72--75},
pmid = {20010604},
publisher = {Nature Publishing Group},
title = {{Preparation and detection of a mechanical resonator near the ground state of motion.}},
url = {http://dx.doi.org/10.1038/nature08681},
volume = {463},
year = {2010}
}
