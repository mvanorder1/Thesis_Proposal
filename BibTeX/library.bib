publisher = {AIP Publishing},
title = {{Pinning effect of the grain boundaries on magnetic domain wall in FeCo-based magnetic alloys}},
url = {http://scitation.aip.org/content/aip/journal/jap/85/9/10.1063/1.370175},
volume = {85},
