@article{Walter1965,
abstract = {Different crystallographic planes, at a gas‐metal interface, have different surface energies. Although the energy difference is small (less than 100 erg/cm2), it is sufficient to cause migration of boundaries between grains of different (hkl) surfaces provided there is a sufficiently small restraint to boundary migration. Since the (110) plane is the plane of lowest surface energy $\gamma$ in 3{\%} silicon‐iron, grains with (110) planes at the gas‐metal interface are favored to grow at the expense of other (hkl) grains ($\gamma$110{\textless}$\gamma$hkl) when the material and annealing atmosphere are free of impurities. In the presence of certain impurities, however, $\gamma$100 may be lower than $\gamma$hkl and grains with (100) planes are favored to grow at the expense of all others. With proper processing, sharp (110)[001] or (100)[001] textures may be obtained in sheets of high‐purity silicon‐iron. Grain size and texture can be altered by deliberate additions of oxide film to the surface, by the amount of silicon in the alloy, and by the choice of annealing atmosphere. The effect of improved texture and low impurity concentration on magnetic properties is discussed.},
author = {Walter, John L.},
doi = {10.1063/1.1714176},
issn = {00218979},
journal = {Journal of Applied Physics},
keywords = {surface energy,texture control},
mendeley-tags = {surface energy,texture control},
month = {jul},
number = {3},
pages = {1213},
publisher = {AIP Publishing},
title = {{Control of Texture in Magnetic Material by Surface Energy}},
url = {http://scitation.aip.org/content/aip/journal/jap/36/3/10.1063/1.1714176},
volume = {36},
year = {1965}
}
@book{Israelachvili2011r,
abstract = {This chapter explores two special interactions, the hydrogen bond and the hydrophobic effect, which are particularly relevant to the interactions of water. Some unusually strong and orientation-dependent bonds are involved in the interactions between water molecules. Hydrogen bonds play a particularly prominent role in water, since each oxygen atom with its two hydrogens can participate in four such linkages with other water molecules: two involving its own H atoms and two involving its unshared (lone-pair) electrons with other H atoms. Computer simulations show that in ice, the molecules order in such a way that each oxygen atom is tetrahedrally coordinated to four other oxygens, with a hydrogen atom lying in the line joining two oxygen atoms. This chapter explains in detail the hydrogen bonding in water molecular. It also introduces models of water and associated liquids. Relative strengths of different types of interactions are also explored in the chapter. The chapter concludes with a discussion on the hydrophotic effect, hydrophotic interaction, and hydrophilic interactions.},
author = {Israelachvili, Jacob N.},
booktitle = {Intermolecular and Surface Forces},
doi = {10.1016/B978-0-12-375182-9.10008-9},
isbn = {9780123751829},
