pages = {27--29},
publisher = {AIP Publishing},
title = {{Promotion of cubic grain growth in 3{\%} silicon iron by control of annealing atmosphere composition}},
url = {http://scitation.aip.org/content/aip/journal/jap/31/5/10.1063/1.1984767},
volume = {408},
year = {1960}
